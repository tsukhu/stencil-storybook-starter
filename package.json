{
  "name": "intermix-design-system",
  "version": "0.0.1",
  "description": "Stencil based design system",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/intermix-design-system/intermix-design-system.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "watch": "stencil build --docs --watch",
    "storybook.run": "start-storybook --port 9001",
    "storybook": "npm-run-all --parallel watch storybook.run"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-typescript": "^7.9.0",
    "@stencil/core": "^1.12.6",
    "@storybook/addon-info": "^5.3.18",
    "@storybook/addon-knobs": "^5.3.18",
    "@storybook/addon-notes": "^5.3.18",
    "@storybook/addon-viewport": "^5.3.18",
    "@storybook/html": "^5.3.18",
    "@types/jest": "^25.2.1",
    "@types/puppeteer": "2.0.1",
    "@types/storybook__addon-knobs": "^5.2.1",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "case": "^1.6.3",
    "glob": "^7.1.6",
    "jest": "24.9.0",
    "jest-cli": "24.9.0",
    "npm-run-all": "^4.1.5",
    "puppeteer": "2.1.1",
    "storybook": "^5.3.18",
    "typescript": "^3.8.3",
    "webpack": "^4.43.0"
  },
  "license": "MIT"
}
